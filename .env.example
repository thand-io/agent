# Environment Variables Example
# Copy this file to .env and modify values as needed

# Server Configuration
THAND_SERVER_HOST=0.0.0.0
THAND_SERVER_PORT=5225

# OIDC Configuration
THAND_OIDC_ISSUER_URL=https://auth.thand.io
THAND_OIDC_CLIENT_ID=your_client_id
THAND_OIDC_CLIENT_SECRET=your_client_secret
THAND_OIDC_JWT_SIGNING_KEY=your_jwt_signing_key

# Logging Configuration
THAND_LOGGING_LEVEL=info
THAND_LOGGING_FORMAT=json

# Security
THAND_SECURITY_CORS_ALLOWED_ORIGINS=https://thand.io,https://*.thand.io

# Workflow Engine
THAND_POLICY_ENGINE_AI_PROVIDER=openai
THAND_POLICY_ENGINE_MODEL=gpt-4

# Provider Configuration
THAND_PROVIDERS_GOOGLE_OAUTH2_CLIENT_ID=your_google_oauth2_client_id
THAND_PROVIDERS_GOOGLE_OAUTH2_CLIENT_SECRET=your_google_oauth2_client_secret
